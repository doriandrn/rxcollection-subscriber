{
  "presets": [
    ["@babel/env", {
      "targets": {
        "node": "7",
        "browsers": ["IE 9"]
      },
      "modules": false
    }],
    "@babel/typescript"
  ],
  "plugins": [
    // "@babel/plugin-transform-runtime",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/proposal-class-properties", { "loose": false }],
    "@babel/proposal-object-rest-spread"
  ]
}
